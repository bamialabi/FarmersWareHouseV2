@model IEnumerable<Core.DataAccess.tbl_MarketInformation>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
             Name
        </th>
        <th>
            Description
        </th>
        <th>
            Quantity 
        </th>
        <th>
            Price
        </th>

        <th>
            Unit
        </th>
        <th>
            Avalability Status
        </th>
        <th>
            Lga  
        </th>
        <th>
            Marketer  
        </th>
        <th>
            Mobile No
        </th><th>
            Market List  
        </th>

        <th>
              State
        </th>
       >
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_MarketProduct.ProductName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuantityForSale)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_MarketUnitOfMeaure.UnitOfMeasureName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AvalabilityStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_Lga.LgaName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_Marketer.MarketerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_Marketer.MobileNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_MarketList.MarketListName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.tbl_LK_State.StateName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.MarketInfoID }) |
                @Html.ActionLink("Details", "Details", new { id=item.MarketInfoID })
                @*|
                    @Html.ActionLink("Delete", "Delete", new { id=item.MarketInfoID })*@
            </td>
        </tr>
}

</table>
